!!!
%html
  %head
    %title Onlinebazar
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body{style: "background: #EEEEEE;"}
    %div{style: "float: right;margin-right: 23px;"}
      .navbar.navbar-fixed-top
        .navbar-inner
          .container
            %a.btn.btn-navbar{"data-target" => ".nav-collapse", "data-toggle" => "collapse"}
              %span.icon-bar
              %span.icon-bar
              %span.icon-bar
            %a.brand{href: "#"}
              = image_tag "main_text.gif"
            .nav-collapse
              %ul.nav
                %li.active
                  %a{href: "#"} Home
                %li.dropdown
                  %a.dropdown-toggle{"data-toggle" => "dropdown", href: "#"}
                    Dropdown
                    %b.caret
                  %ul.dropdown-menu
                    %li
                      %a{href: "#"} Item 1
                    %li
                      %a{href: "#"} Item 2
                / dropdown
                = render :partial => "/homes/my_account"
              / /.nav
            / /.nav-collapse
          / /.container
        / /.navbar-inner
      / /.navbar
    = yield