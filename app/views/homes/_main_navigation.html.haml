%nav.navbar.navbar-default{role: "navigation"}
  .navbar-header
    %ul.nav.navbar-nav
      - @categories.each do |category|
        %li.dropdown
          %a.dropdown-toggle{"data-toggle" => "dropdown", href: "#"}
            = category.name
          %ul.dropdown-menu
            - category.sub_categories.each do |subcategory|
              %li
                %a
                  = subcategory.name
  / /.navbar-collapse